{
  "name": "mjml-mailer-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --watch '.env' --exec 'ts-node' src/server.ts",
    "run": "NODE_ENV=production npm run build && npm run start",
    "xrun": "ts-node --project tsconfig.json src/server.ts",
    "build": "tsc",
    "scrub": "rm -rf node_modules/ dist/",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lintfix": "eslint --fix --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "email-addresses": "^5.0.0",
    "express": "^4.21.2",
    "express-cors": "^0.0.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "nunjucks": "^3.2.4",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "synchronized-promise": "^0.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mjml": "^4.7.4",
    "@types/nodemailer": "^6.4.15",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.33.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vue-eslint-parser": "^10.1.3"
  }
}
